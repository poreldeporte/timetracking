<h1>Show a Project</h1>

<% if @projects.id != nil %>
	<strong>ID:</strong> <%= @projects.id %>
	<br>
<% end %>

<% if @projects.name != nil %>
	<strong>Name:</strong> <%= @projects.name %>
	<br>
<% end %>

<% if @projects.created_at != nil %>
	<strong>Created at:</strong> <%= @projects.created_at %>
	<br>
<% end %>

<% if @projects.updated_at != nil %>
	<strong>Updated at:</strong><%= @projects.updated_at %>
	<br>
<% end %>

<% if @projects.estimate != nil %>
	<strong>Estimate:</strong> <%= @projects.estimate %>
	<br>
<% end %>

<% if @projects.description != nil %>
	<strong>Description:</strong> <%= @projects.description %>
<% end %>